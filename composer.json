{
    "name": "topdigital/laravel-content",
    "description": "Laravel Content module",
    "authors": [
        {
            "name": "Sergey Sobolev",
            "email": "soberzerg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "topdigital/laravel-auth": "dev-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TopDigital/laravel-auth.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TopDigital\\Content\\ContentServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "TopDigital\\Content\\": "src/"
        }
    }
}
